.music-ranking
    width 100%
    height 100%
    background-image: linear-gradient(177deg, #000000 0%, #343038 100%)
    .ranking-list
        .ranking-wrapper
            display flex
            .left
                flex 0 0 2rem
                width 2rem
                height 2rem
                img
                    width 100%
                    height 100%
            .right
                font-size 14px
                flex 1
                line-height 0.44rem
                padding 0 .2rem
                color #e2509d
                overflow hidden
                .ranking-title
                    font-size 16px
                    margin 5px 0
                .top-song
                    .name
                        margin 0 5px


