.music-header
    position fixed
    width 100%
    font-size 20px
    color #e2509d
    height 55px
    line-height 55px
    text-align center
    z-index 100
    .header-back
        position absolute
        left 0.2rem
        font-size 22px
    .header-title
        margin: 0 40px
        letter-spacing 3px
        overflow: hidden
        text-overflow: ellipsis
        white-space: nowrap
